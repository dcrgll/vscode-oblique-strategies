{
  "name": "oblique-stratergies",
  "displayName": "oblique-stratergies",
  "description": "A Code extension which displays a random oblique stratergy in the status bar.",
  "galleryBanner.color": "292929",
  "version": "1.0.0",
  "publisher": "dcrgll",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oblique-stratergies.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.14.0",
    "glob": "^7.2.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
