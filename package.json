{
  "name": "oblique-stratergies",
  "displayName": "oblique-stratergies",
  "description": "A Code extension which displays a random oblique stratergy in the status bar.",
  "galleryBanner.color": "292929",
  "version": "1.0.0",
  "publisher": "dcrgll",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oblique-stratergies.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.76.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.38.0",
    "glob": "^9.3.2",
    "husky": "^8.0.3",
    "mocha": "^10.6.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
